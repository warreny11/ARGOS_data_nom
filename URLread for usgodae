bsize = size(b);
csize = size(c);
dsize = size(d);
esize = size(e);
fsize = size(f);
gsize = size(g);
hsize = size(h);
isize = size(i);
jsize = size(j);
ksize = size(k);

alinecharnum = a(2) - a(1);
