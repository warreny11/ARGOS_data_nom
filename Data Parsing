

BEGIN {
 FS = ","

}

{c = NF
   
while (FNR==NR) {

for (j = 1; j <= c; j++) {
X[$j] = $j

}
b=length(X)

next
}




{ 

c = NF - 1 
     

    for (i = 1; i <= c; i++) { #for every field in line
      
 
              (split($i, a, "=")) #split field by eq sign


                
if (a[1] in X) {
                    
                     print a[2] #a[2] has just data
}

        }
         
    }


}
